# Create Vlan and name them:

Switch (config)#vlan 100
Switch (config-vlan)#name Developer
Switch (config-vlan)#vlan 200
Switch (config-vlan) #name HR
Switch (config-vlan)#exit

# Assign PC ports :

Switch (config)#int range fa0/1-2
Switch (config-if-range) #switchport mode access
Switch (config-if-range)#switchport access vlan 100
Switch (config-if-range) #exit

Switch (config)#int range fa0/3-4
Switch (config-if-range) #sw mode access
Switch (config-if-range)#sw access vlan 200

# Trunk port to Router:

Switch (config)#int fa0/5
Switch (config-if) #sw mode trunk     // Note: sw is short form of switchport
Switch (config-if)#no shutdown
Switch (config-if) #exit
